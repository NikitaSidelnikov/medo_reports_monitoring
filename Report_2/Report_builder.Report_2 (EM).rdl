    <DataSet Name="Report1">
      <Query>
        <DataSourceName>MedonomFull</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DataStartCompare">
            <Value>=Parameters!DataStartCompare.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DataEndCompare">
            <Value>=Parameters!DataEndCompare.Value</Value>
