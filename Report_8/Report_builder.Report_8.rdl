    <DataSet Name="GetCountPackages">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DataStart">
            <Value>=Parameters!DataStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DataEnd">
            <Value>=Parameters!DataEnd.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Member">
            <Value>=Parameters!Member.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	COUNT(*) AS PackagesCount
FROM (
	SELECT -- SuccesPackage = все валидные логи по всем обработанным пакетам
		Package.Batch
	FROM
		ValidationLog
	INNER JOIN Package
		ON Package.Id = ValidationLog.Package
	WHERE 
		ValidationLog.Success = 1
		AND Package.Processed = 1
		AND Incoming = 0
		AND Package.ReceivedOn &gt;=  IIF(@DataStart is null, Package.ReceivedOn, DATETIMEFROMPARTS(DATEPART(YEAR, @DataStart), DATEPART(MONTH, @DataStart), DATEPART(DAY, @DataStart), '0', '0', '0', '0')) --начало периода отчета
		AND Package.ReceivedOn &lt;= IIF(@DataEnd is null, Package.ReceivedOn, DATETIMEFROMPARTS(DATEPART(YEAR, @DataEnd), DATEPART(MONTH, @DataEnd), DATEPART(DAY, @DataEnd), '23', '59', '59', '0')) --конец периода отчета
		--AND Package.ReceivedOn &gt;=  '2021-04-01' --начало периода отчета
		--AND Package.ReceivedOn &lt; '2021-04-01' --конец периода отчета
) AS SuccesPackage
INNER JOIN Batch
	ON Batch.Id = SuccesPackage.Batch
INNER JOIN Member
	ON Member.Guid = Batch.MemberGuid
WHERE
	Member.Name = @Member
	--Batch.MemberGuid = 'ecc99082-f3b4-4028-8007-a7f69d6cd1d7' --АО ДОМ.РФ</CommandText>
      </Query>
      <Fields>
        <Field Name="PackagesCount">
          <DataField>PackagesCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
