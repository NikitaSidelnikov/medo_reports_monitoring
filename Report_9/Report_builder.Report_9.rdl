    <DataSet Name="GetPackages">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Member">
            <Value>=Parameters!Member.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DataStart">
            <Value>=Parameters!DataStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DataEnd">
            <Value>=Parameters!DataEnd.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	Package.Id
FROM 
	Member
INNER JOIN Subscriber
	ON Subscriber.Member = Member.Id
INNER JOIN Batch
	ON Batch.Subscriber = Subscriber.Id
INNER JOIN Package
	ON Package.Batch = Batch.Id --находим Package через Batch (через score не делаем, ибо там много записей и очень долго обрабатывается)

INNER JOIN  (
	SELECT -- SuccesDate - актуальные валидные пакеты. Если логи по пакету менялись, то берем последний обработанный лог пакета
		SuccesLogs.Package				AS Package
		,MAX(SuccesLogs.ValidatedOn)	AS Max_ValidatedOn
	FROM (	
		SELECT -- SuccesLogs = логи, которые были успешно обработаны (succes=1)
			Package
			,ValidatedOn
		FROM
			ValidationLog
		WHERE 
			Success = 1
	) AS SuccesLogs

	GROUP BY
		SuccesLogs.Package
) AS SuccesDate
	ON SuccesDate.Package = Package.Id

WHERE 
	Member.Name = @Member
	--Member.Name = N'АО ДОМ.РФ'
	AND Package.Processed = 1
	AND Package.Incoming = 0 --смотрим только валидные и обработанные исходящие пакеты по участнику
	AND Package.ReceivedOn &gt;=  case when @DataStart is not null
									then DATETIMEFROMPARTS(DATEPART(YEAR, @DataStart), DATEPART(MONTH, @DataStart), DATEPART(DAY, @DataStart), '0', '0', '0', '0') --начало периода отчета
									else Package.ReceivedOn
									end
	AND Package.ReceivedOn &lt;= case when @DataEnd is not null
									then DATETIMEFROMPARTS(DATEPART(YEAR, @DataEnd), DATEPART(MONTH, @DataEnd), DATEPART(DAY, @DataEnd), '0', '0', '0', '0') --начало периода отчета
									else Package.ReceivedOn
									end
</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
