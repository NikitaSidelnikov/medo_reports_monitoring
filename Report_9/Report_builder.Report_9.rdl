    <DataSet Name="GetMember">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
	Name
FROM 
	Member
ORDER BY Name</CommandText>
      </Query>
      <Fields>
        <Field Name="Name">
